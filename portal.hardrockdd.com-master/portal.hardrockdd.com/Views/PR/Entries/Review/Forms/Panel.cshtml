@model portal.Models.Views.Payroll.Review.PayrollEmployeeReviewFormViewModel
@{
    var id = Guid.NewGuid().ToString().Replace("-", "");
    var panelId = string.Format("panel_{0}", Guid.NewGuid());
}
<div class="panel panel-inverse"
     data-sortable-id="PayrollEmployeeReviewFormViewModel"
     data-reloadurl="@Url.Action("Form", "PayrollEmployeeEntryForm", new { prco = Model.PRCo, employeeId = Model.EmployeeId, weekId = Model.WeekId })">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
        </div>
        <h4 class="panel-title">@Model.Employee.EmployeeName (@Model.EmployeeId)</h4>
    </div>
    <div class="panel-body" id="@panelId">
        @Html.Partial("../PR/Entries/Review/Forms/Form", Model)
    </div>
</div>