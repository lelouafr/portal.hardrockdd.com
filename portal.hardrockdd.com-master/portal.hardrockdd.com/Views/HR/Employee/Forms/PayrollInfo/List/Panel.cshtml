@model portal.Models.Views.Payroll.Leave.LeaveRequstFormViewModel
@{
    var panelId = string.Format("panel_{0}", Guid.NewGuid());
    var id = panelId.ToString().Replace("-", "");
}

<div class="panel panel-inverse"
     data-sortable-id="LeaveRequestLineListViewModel"
     id="LeaveRequestLineListViewModel"
     data-reloadurl="@Url.Action("Table", "LeaveRequestLine", new { prco = Model.Request.PRCo, requestId = Model.Request.RequestId })"
     data-reloadpanels="LeaveEmployeeSummaryListViewModel_Panel"
     >     
    <div class="panel-heading">
        <div class="panel-heading-btn">
            
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
            
        </div>
        <h4 class="panel-title">Time Off Line Items</h4>
    </div>
    <div class="panel-body" id="@panelId">
        @Html.Partial("../LeaveRequest/Lines/Table", Model.Lines)
    </div>
</div>

<script>
</script>
