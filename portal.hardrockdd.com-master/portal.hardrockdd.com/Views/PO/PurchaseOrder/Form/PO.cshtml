@model portal.Models.Views.Purchase.Order.PurchaseOrderViewModel

@{
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    bool partialView = bool.TryParse(ViewBag.Partial?.ToString(), out bool partialViewOut) ? partialViewOut : false;
}


<div class="modal-body">
    <div class="row"
            data-poco="@Model.POCo"
            data-requestid="@Model.PO">
        <div class="col-lg-12 m-b-10">
            <div class="f-s-18">PO: @Model.PO</div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <div class="pull-right">
        <a href="javascript:;" class="btn btn-white" data-dismiss="modal">Close</a>
    </div>
</div>