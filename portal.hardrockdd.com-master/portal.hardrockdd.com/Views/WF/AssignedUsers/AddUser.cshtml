@model portal.Models.Views.WF.WorkFlowUserViewModel
@{
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    var formLayout = new TableLayoutModel(this);
    var entityKey = formLayout.TableKey;
    var formId = "form_" + Guid.NewGuid();
}

<form class="form-horizontal"
      data-entitykey="@entityKey"
      data-urlcreate="@Url.Action("AddUser", "WorkFlow")">
    <div class="form-group row">
        <div class="col-10">
            @Html.HiddenFor(m => m.WFCo, new { @id = Guid.NewGuid() })
            @Html.HiddenFor(m => m.WorkFlowId, new { @id = Guid.NewGuid() })
            @Html.HiddenFor(m => m.SeqId, new { @id = Guid.NewGuid() })
            @Html.EditorFor(m => m.AssignedUser)
        </div>
        <div class="col-2">
            <div class="pull-right">
                <button type="button" class="btn btn-primary m-r-5 addUser">Add</button>
            </div>
        </div>
    </div>
</form>
<script>
    $('#@formId').ready(function () { Form().init($('#@formId')); });
</script>