@model portal.Models.Views.JC.Job.JobListViewModel
<div class="panel panel-inverse"
     data-sortable-id="@(Guid.NewGuid())"
     data-reloadurl="@Url.Action("SearchTable", "JobCombo", new { Co = Model.Co })"
      style="margin: 0px; padding: 0px;">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
        </div>
        <h4 class="panel-title">Job Lookup</h4>
    </div>
    <div class="panel-body" id="JobListViewModel"
          style="margin: 0px; padding: 10px 5px 10px 5px;">
        @Html.Partial("../JC/Job/Search/Table", Model)
    </div>
</div>

<script>
    $(document).ready(function(){
        handleTableAutoUpdate();
    });
</script>