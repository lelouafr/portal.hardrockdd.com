@model portal.Models.Views.Bid.BidPackageCostListViewModel
@{
    var panelId = string.Format("panel_{0}", Guid.NewGuid());
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
}
<div class="panel panel-inverse"
     data-reloadurl="@Url.Action("Table", "BidPackageCost", new { co = Model.Co, bidId = Model.BidId , packageId = Model.PackageId })"
     data-reloadpanels="BidBoreLineCostItemListViewModel">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
            
        </div>
        <h4 class="panel-title">Allocated Cost Items</h4>
    </div>
    <div class="panel-body" id="@panelId">
        @Html.Partial("../BD/Bid/Package/AllocatedCost/List/Table", Model)
    </div>
</div>
<script>

</script>
