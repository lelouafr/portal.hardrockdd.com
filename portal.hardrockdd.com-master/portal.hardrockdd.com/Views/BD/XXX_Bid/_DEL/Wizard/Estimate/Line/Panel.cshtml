@model portal.Models.Views.Bid.Wizard.Estimate.BidBoreLineListViewModel

<div class="panel panel-inverse" 
     id="BidBoreLineListViewModel"
     data-reloadurl="@Url.Action("Table", "BidBoreLineEstimateWizard", new { co = Model.Co, bidId = Model.BidId })"
     data-reloadpanels="">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
            
        </div>
        <h4 class="panel-title">Bore Lines</h4>
    </div>
    <div class="panel-body" id="EstimateBoreLines">
        @Html.Partial("../BidWizard/Estimate/BidBoreLine/Table", Model)
    </div>
</div>
