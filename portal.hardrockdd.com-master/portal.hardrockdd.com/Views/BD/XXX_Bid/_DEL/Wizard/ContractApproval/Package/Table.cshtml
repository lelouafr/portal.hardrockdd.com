@model  portal.Models.Views.Bid.Wizard.ContractApproval.PackageListViewModel
@{
    ViewBag.TableRow = "True";
    var tableId = string.Format("table_{0}", Guid.NewGuid());
    var tableLayout = new TableLayoutModel(this);
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    string tableKey = "";
    foreach (var prp in tableLayout.Properties)
    {
        if (prp.IsKey)
        {
            var myData = Model.GetType().GetProperty(prp.Property);
            var myVal = myData.GetValue(Model, null);
            if (tableKey != string.Empty)
            {
                tableKey += "&";
            }
            tableKey += prp.Property + "=" + myVal.ToString();
        }
    }
}

<table class="table"
       id="@tableId"
       data-urlupdate="@Url.Action("Update", "BidPackageContractApprovalWizard")"
       data-urlvalidate="@Url.Action("Validate", "BidPackageContractApprovalWizard")"
       data-urlselect="@Url.Action("Panel", "BidBoreLineContractApprovalWizard")"
       data-dstselect="BidBoreLineSelect"
       data-dstselectvalidate="false"
       data-tablekey="@tableKey">
    <thead>
        <tr class="">
            <th width="20%">@Html.LabelForRequired(model => model.List.FirstOrDefault().Description)</th>
            <th width="12%">@Html.LabelForRequired(model => model.List.FirstOrDefault().DivisionId)</th>
            <th width="16%">@Html.LabelForRequired(model => model.List.FirstOrDefault().CustomerId)</th>
            <th width="10%">@Html.LabelForRequired(model => model.List.FirstOrDefault().CustomerReference)</th>
            <th width="14%">@Html.LabelForRequired(model => model.List.FirstOrDefault().JobId)</th>
            <th width="8%">@Html.LabelForRequired(model => model.List.FirstOrDefault().Pricing.DirtLFPrice)</th>
            <th width="8%">@Html.LabelForRequired(model => model.List.FirstOrDefault().Pricing.RockLFPrice)</th>
            <th width="10%">@Html.LabelForRequired(model => model.List.FirstOrDefault().Status)</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model.List)
        {
            @Html.Partial("../BD/Bid/Wizard/ContractApproval/Package/TableRow", obj)
        }
    </tbody>
</table>
<script>

</script>