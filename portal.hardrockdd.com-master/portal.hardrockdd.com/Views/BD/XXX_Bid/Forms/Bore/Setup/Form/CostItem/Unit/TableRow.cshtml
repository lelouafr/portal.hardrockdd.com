@model portal.Models.Views.Bid.Forms.Bore.Setup.CostItemViewModel
@{
    var tableLayout = new TableLayoutModel(this);
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    var entityKey = tableLayout.TableKey;
}
<tr data-entitykey="@entityKey">
    <td>
        @Html.AntiForgeryToken()
        @Html.HiddenFor(m => m.BDCo, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.BidId, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.BoreId, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.RockLineId, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.DirtLineId, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.Cost, new { @id = Guid.NewGuid() })
        @Html.EditorFor(m => m.BudgetCodeId, new { ViewOnly = true })
    </td>
    <td>
        @Html.EditorFor(m => m.Multiplier)
    </td>
    <td>
        @Html.EditorFor(m => m.DirtUnits, new { ViewOnly = true })
    </td>
    <td>
        @Html.EditorFor(m => m.RockUnits, new { ViewOnly = true })
    </td>
    <td>
        @Html.EditorFor(m => m.UM, new { ViewOnly = true })
    </td>
</tr>


