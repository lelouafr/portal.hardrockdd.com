@model portal.Models.Views.Bid.Forms.Package.Price.PriceViewModel
@{
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    var formLayout = new TableLayoutModel(this);
    var entityKey = formLayout.TableKey;
    var formId = string.Format("form_{0}", Guid.NewGuid());
}
<form class="form-horizontal"
      id="@formId"
      data-entitykey="@entityKey"
      data-urlupdate="@Url.Action("PackagePriceUpdate", "BidForm")"
      data-urlvalidate="@Url.Action("PackagePriceValidate", "BidForm")">
    @Html.AntiForgeryToken()
    <div class="form-group has-feedback">
        <div class="validation-summary-valid text-danger" data-valmsg-summary="true">
        </div>
    </div>
    @Html.HiddenFor(m => m.BDCo, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.BidId, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.PackageId, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.Status, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.Description, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.BoreTypeId, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.NumberOfBores, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.PipeSize, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.RigCategoryId, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.GroundDensityId, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.MarketId, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.IndustryId, new { @id = Guid.NewGuid() })
    @Html.HiddenFor(m => m.DivisionId, new { @id = Guid.NewGuid() })

    <div class="row">
        <div class="col-12">

            <div class="form-group has-feedback row">
                @Html.EditorFor(m => m.DirtBidDays)
                @Html.EditorFor(m => m.RockBidDays)
            </div>
            <div class="form-group has-feedback row">
                @Html.EditorFor(m => m.DirtCost)
                @Html.EditorFor(m => m.RockCost)
            </div>
            <div class="form-group has-feedback row">
                @Html.EditorFor(m => m.DirtLFCost)
                @Html.EditorFor(m => m.RockLFCost)
            </div>
            <div class="form-group has-feedback row">
                @Html.EditorFor(m => m.DirtGM)
                @Html.EditorFor(m => m.RockGM)
            </div>
            <div class="form-group has-feedback row">
                @Html.EditorFor(m => m.DirtLFPrice)
                @Html.EditorFor(m => m.RockLFPrice)
            </div>
            <div class="form-group has-feedback row">
                <div class="col-6"></div>
                @Html.EditorFor(m => m.RockRevenueAdder)
            </div>
            <div class="form-group has-feedback row">
                @Html.EditorFor(m => m.DirtRevenue)
                @Html.EditorFor(m => m.RockRevenue)
            </div>
            <div class="form-group has-feedback row">
                @Html.EditorFor(m => m.DirtProfit)
                @Html.EditorFor(m => m.RockProfit)
            </div>
        </div>
    </div>
</form>