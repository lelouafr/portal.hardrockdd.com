@model portal.Models.Views.Dashboard.HomeViewModel
<div class="col-xl-2 col-lg-3 col-md-4">
    <!-- begin card -->
    <div class="card card-inverse bg-black-lighter">
        <div class="card-block">
            <h4 class="card-title m-t-5 m-b-10">New Ticket</h4>
            <p class="card-text m-b-15">Generate new daily ticket.</p>
            <button class="btn btn-sm btn-primary addTicket" data-toggle="modal" data-target="#addTicket">
                Create Ticket
            </button>
        </div>
    </div>
</div>