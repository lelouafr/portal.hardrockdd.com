@model portal.Models.Views.Dashboard.HomeViewModel
@if (User.HasAccess("ServiceRequest"))
{
    <!-- end col-3 -->
    <div class="col-xl-2 col-lg-3 col-md-4">
        <!-- begin card -->
        <div class="card card-inverse bg-gradient-teal">
            <div class="card-block">
                <h4 class="card-title m-t-5 m-b-10">Service Request</h4>
                <p class="card-text m-b-15">Request Service on Equipment.</p>
                <a href="@Url.Action("CreateServiceRequest","ServiceRequest")" class="btn btn-sm btn-warning">Create Request</a>
            </div>
        </div>
    </div>
}