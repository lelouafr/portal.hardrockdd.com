@model List<portal.Models.Views.DailyTicket.DailyHolidayEntryViewModel>

@for (int i = 0; i < Model.Count; i++)
{
    if (i == 0)
    {
        @Html.Partial("../DT/Type/Holiday/Entry/List/Row/TableRowFirst", Model[i])
    }
    else
    {
        @Html.Partial("../DT/Type/Holiday/Entry/List/Row/TableRow", Model[i])
    }
    @Html.Partial("../DT/Type/Holiday/Entry/List/Row/TableCommentRow", Model[i])
}