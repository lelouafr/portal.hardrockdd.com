@model portal.Models.Views.DailyTicket.DailyEquipmentListViewModel


<div class="panel panel-inverse" data-sortable-id="DailyEquipmentListViewModel"
     data-reloadurl="@Url.Action("Table", "DailyEquipment", new { dtco = Model.DTCo, ticketId = Model.TicketId, forEdit = !ViewBag.ViewOnly })">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-url="@Url.Action("Panel","AuditLog")" data-click="panel-history" style="display:none"><i class="fas fa-history"></i></a>
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
        </div>
        <h4 class="panel-title">Equipment List</h4>
    </div>
    <div class="panel-body" id="equipment">
        @Html.Partial("../DT/Type/Job/Equipment/List/Table", Model)
    </div>
</div>