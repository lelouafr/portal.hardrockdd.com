@model portal.Models.Views.HQ.Batch.PR.PRPostedTransViewModel
@{
    var tableLayout = new TableLayoutModel(this);
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    var entityKey = tableLayout.TableKey;
}
<tr data-entitykey="@entityKey">
    <td>
        @Html.AntiForgeryToken()
        @Html.HiddenFor(m => m.PRCo, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.BatchId, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.BatchSeq, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.JCCo, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.PhaseGroupId, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.EMCo, new { @id = Guid.NewGuid() })
    </td>
    <td class="f-s-10">
        @Model.EmployeeName
    </td>
    <td>
        @Model.PostDate.ToShortDateString()
    </td>
    <td class="f-s-9">
        @Model.JobName
    </td>
    <td class="f-s-9">
        @Model.PhaseName
    </td>
    <td class="f-s-9">
        @Model.EquipmentName
    </td>
    <td class="f-s-9">
        @Model.EMCostCodeName
    </td>
    <td class="f-s-9">
        @Model.CrewName
    </td>
    <td>
        @Model.EarnCodeDescription
    </td>
    <td class="text-right">
        @Model.Hours
    </td>
    <td class="text-right">
        @(Math.Round(Model.Rate, 2))
    </td>
    <td class="text-right">
        @Model.Amt
    </td>
</tr>
