@model portal.Models.Views.AP.Document.DocumentFormViewModel
@{
    var id = Guid.NewGuid().ToString().Replace("-", "");
}
<div class="panel panel-inverse"
     data-sortable-id="documentReview"
     data-reloadurl="@Url.Action("ReviewFrom", "APDocument", new { apco = Model.Document.APCo, docId = Model.Document.DocId })">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
            
        </div>
        <h4 class="panel-title">Review</h4>
    </div>
    <div class="panel-body" id="documentReview">
        @Html.Partial("../AP/Document/Review/Form", Model)
    </div>
</div>