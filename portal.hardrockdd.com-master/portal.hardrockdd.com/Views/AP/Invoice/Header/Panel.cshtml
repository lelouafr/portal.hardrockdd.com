@model portal.Models.Views.AP.Invoice.InvoiceFormViewModel
@{
    var id = Guid.NewGuid().ToString().Replace("-", "");
    var panelId = string.Format("panel_{0}", Guid.NewGuid());
}
<div class="panel panel-inverse"
     data-sortable-id="PurchaseOrderViewModel"
     data-reloadurl="@Url.Action("HeaderForm", "APInvoice", new { apco = Model.Invoice.APCo, mth = Model.Invoice.Mth, APTransId = Model.Invoice.APTransId })"
     data-reloadpanels="PurchaseOrderItemViewModel">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-url="@Url.Action("Panel","AuditLog")" data-click="panel-history" style="display:none"><i class="fas fa-history"></i></a>            
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
        </div>
        <h4 class="panel-title">Info (@Model.Invoice.APTransId)</h4>
    </div>
    <div>
    </div>
    <div class="panel-body" id="@panelId">
        @Html.Partial("../AP/Invoice/Header/Form", Model.Invoice)
    </div>
</div>