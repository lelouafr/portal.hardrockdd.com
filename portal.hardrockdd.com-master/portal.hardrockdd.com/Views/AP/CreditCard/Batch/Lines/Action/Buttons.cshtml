@model portal.Models.Views.AP.CreditCard.Batch.Form.ActionViewModel

@{
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    var unSubmitAllowed = bool.TryParse(ViewBag.UnSumbitAllowed?.ToString(), out bool unSumbitAllowedOut) ? unSumbitAllowedOut : false;
    bool partialView = bool.TryParse(ViewBag.Partial?.ToString(), out bool partialViewOut) ? partialViewOut : false;
}

<div class="row"
     data-co="@Model.CCCo"
     data-mth="@Model.Mth"
     data-batchid="@Model.BatchId"
     data-source="@Model.Source">
    <div class="col-lg-12 m-b-10">
        @Html.AntiForgeryToken()
        <div class="pull-right">
            @if (Model.CanAddtoBatch)
            {
                <button type="button" class="btn btn-success btnAction"
                        data-url="@Url.Action("CreateBatch", "APCreditCardBatch")"
                        data-co="@Model.CCCo"
                        data-mth="@Model.Mth"
                        data-batchid="@Model.BatchId"
                        data-source="@Model.Source">
                    <i class="far fa-share-square"></i> Send to Batch
                </button>
            }
        </div>
    </div>
</div>

<script>
    $(document).ready(function(){
        $(document)
            .off('click', 'button.btnAction')
            .on('click', 'button.btnAction', function () {
                CallBtnAction(this, function () {

                });
            });
    });
</script>