@model portal.Models.Views.AP.CreditCard.Batch.Lines.FormViewModel
@{
    var panelId = "lines" + Guid.NewGuid();
}
<div class="accordion">
    <!-- begin card -->
    <div class="card" style="overflow:visible;">
        <div class="card-header bg-dark-darker pointer-cursor d-flex align-items-center" data-toggle="collapse" data-target="#@panelId">
            <i class="fa fa-circle text-blue  mr-2 f-s-8"></i>  Batch Details
        </div>
        <div id="@panelId" class="show">
            <div class="card-body">
                @Html.Partial("../AP/CreditCard/Batch/lines/List/Table", Model.Lines)
            </div>
        </div>
    </div>
    <!-- end card -->
</div>