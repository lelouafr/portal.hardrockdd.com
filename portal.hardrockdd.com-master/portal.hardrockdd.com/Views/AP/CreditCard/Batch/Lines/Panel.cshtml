@model portal.Models.Views.AP.CreditCard.Batch.Lines.FormViewModel
@{
    var id = Guid.NewGuid().ToString().Replace("-", "");
    var panelId = string.Format("panel_{0}", Guid.NewGuid());
}
<div class="panel panel-inverse"
     data-reloadurl="@Url.Action("Form", "APCreditCardBatchLine", new { ccco = Model.CCCo, Mth = Model.Mth, BatchId = Model.BatchId, Source = Model.Source })">
    <div class="panel-heading">
        <div class="panel-heading-btn">           
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
        </div>
        <h4 class="panel-title">Batch (@Model.BatchId)</h4>
    </div>
    <div class="panel-body" id="@panelId">
        @Html.Partial("../AP/CreditCard/Batch/Lines/Form", Model)
    </div>
</div>