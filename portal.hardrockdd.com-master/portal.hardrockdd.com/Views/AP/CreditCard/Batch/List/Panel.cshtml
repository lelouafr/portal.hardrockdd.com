@model portal.Models.Views.AP.CreditCard.Batch.FormViewModel
@{
    var panelId = string.Format("panel_{0}", Guid.NewGuid());
    var id = panelId.ToString().Replace("-", "");

}
<div class="panel panel-inverse"
     id="@panelId"
     style="margin-bottom: 0px;"
     data-reloadurl="@Url.Action("TransTable", "APCreditCardBatch", new { co = Model.Co, mth = Model.Mth })">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
        </div>
        <h4 class="panel-title">Credit Card Batches</h4>
    </div>
    <div class="panel-body"
         style="padding-bottom: 5px;"
         id="@panelId">
        @Html.Partial("../AP/CreditCard/Batch/List/Table", Model.Batches)
    </div>
</div>