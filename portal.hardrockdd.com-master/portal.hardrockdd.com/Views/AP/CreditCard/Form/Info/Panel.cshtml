@model portal.Models.Views.AP.CreditCard.Form.InfoViewModel
@{
    var panelId = "info";
}
<div class="accordion">
    <!-- begin card -->
    <div class="card" style="overflow:visible;">
        <div class="card-header bg-dark-darker pointer-cursor d-flex align-items-center" data-toggle="collapse" data-target="#@panelId">
            <i class="fa fa-circle fa-fw text-blue mr-2 f-s-8"></i>  Info
        </div>
        <div id="@panelId" class="collapse show">
            <div class="card-body">
                @Html.Partial("../AP/CreditCard/Form/Info/Form", Model)
            </div>
        </div>
    </div>
    <!-- end card -->
</div>