@model portal.Models.Views.AP.Vendor.Forms.VendorFormViewModel

<style>
    .accordion > .card {
        overflow: unset;
    }

    .card {
        margin-bottom: 5px;
        overflow: visible;
    }

    .card-body {
        padding: 0px;
        overflow: visible;
    }
</style>

<div class="row">
    <div class="col-6">
        @Html.Partial("../AP/Vendor/Forms/Info/Panel", Model.Info)
    </div>
    <div class="col-6">
        @Html.Partial("../AP/Vendor/Forms/Address/Panel", Model.Info)
        @Html.Partial("../AP/Vendor/Forms/Merchant/Panel", Model.Merchant)
    </div>
</div>

@Html.Partial("../AP/Vendor/Forms/Coding/Panel", Model.Coding)
@Html.Partial("../AP/Vendor/Forms/Invoices/Panel", Model.Invoices)