@model portal.Models.Views.Vendor.VendorListViewModel
<div class="panel panel-inverse"
     
     data-reloadurl="@Url.Action("TableSearch", "VendorCombo")"
     style="padding: 0px;">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
        </div>
        <h4 class="panel-title">Vendor Lookup</h4>
    </div>
    <div class="panel-body" id="VendorListViewModel" style="padding: 10px 5px 10px 5px;">
        @Html.Partial("../AP/Vendor/Search/Table", Model)
    </div>
</div>