@model portal.Models.Views.EM.WorkOrder.Item.WorkOrderItemViewModel
@{
    var tableLayout = new TableLayoutModel(this);
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    var entityKey = "";
    ViewBag.TableRow = "True";

    entityKey = tableLayout.TableKey;
}
<tr data-entitykey="@entityKey">
    <td>
        @Html.AntiForgeryToken()
        @Html.HiddenFor(m => m.EMCo, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.WorkOrderId, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.WorkOrderItemId, new { @id = Guid.NewGuid() })
        @Model.WorkOrderItemId
    </td>
    <td>
        @Html.EditorFor(m => m.EquipmentId)
    </td>
    <td>
        @Html.EditorFor(m => m.Description)
    </td>
    <td>
        @if (Model.DateDue.HasValue)
        {
            @Model.DateDue.Value.ToString("MM/dd/yyyy")
        }
    </td>
    <td>
        @if (Model.Complete)
        {
            <span class="badge badge-success">Yes</span>
        }
        else
        {
            <span class="badge badge-secondary">No</span>
        }
    </td>
    <td>
        <button class="btn btn-sm btn-danger pull-right delRow" type="button"
                data-entitykey="@entityKey">
            <i class="fa fa-trash"></i>
        </button>
    </td>
</tr>