@model portal.Models.Views.Equipment.Forms.EquipmentInfoViewModel
@{
    var id = Guid.NewGuid().ToString().Replace("-", "");
    var panelId = "panelId_" +  Guid.NewGuid();
}

<div class="card" style="overflow:visible;">
    <div class="card-header bg-dark-darker pointer-cursor d-flex align-items-center" data-toggle="collapse" data-target="#@panelId">
        <i class="fa fa-circle fa-fw text-blue mr-2 f-s-8"></i> Equipment Info
    </div>
    <div id="@panelId" class="collapse show">
        <div class="card-body">
            @Html.Partial("../EM/Equipment/Forms/Info/Form", Model)
        </div>
    </div>
</div>