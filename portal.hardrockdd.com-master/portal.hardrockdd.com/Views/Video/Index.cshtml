@section styles {
    @Styles.Render("~/csspage/inputcontrols")
    <style>
        .content {
            padding: 0px !important;
        }

        .panel-body {
            padding: 0px !important;
        }
    </style>
}
<iframe id="preview" src="https://video.ui.com/" frameborder="0" style="width: 100%; height:100%;" allowfullscreen>
</iframe>

@section scripts {

    @Scripts.Render("~/jspage/inputcontrols")
    <script>
        $(document).ready(function () {
            var h = $("body").height();
            h -= $("#header").height();
            h -= 4;
            $("#preview").height(h);
        });
    </script>
}