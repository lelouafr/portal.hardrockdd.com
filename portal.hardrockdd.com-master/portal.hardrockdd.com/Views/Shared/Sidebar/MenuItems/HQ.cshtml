
@{
    var listItemId = 1;
    var listItemTag = "hq";
}
@if (User.HasAccess("Batch", "BatchListIndex"))
{
    <li id="menu_li_@(listItemTag)_@(listItemId++)" class="has-sub">
        <a href="javascript:;">
            <b class="caret"></b>
            <i class="fas fa-list-ol"></i>
            <span>Batches</span>
        </a>
        <ul class="sub-menu">
            <li id="menu_li_@(listItemTag)_@(listItemId++)"><a href="@Url.Action("BatchListIndex", "Batch", routeValues: new { Area = "" })">Batches</a></li>
        </ul>
    </li>
}

