@model portal.Models.Views.Purchase.Order.PurchaseOrderSummaryListViewModel
<div class="panel panel-inverse"
     
     data-reloadurl="@Url.Action("TableSearch", "PO", new { co = @Model.POCo, vendorId = @Model.VendorId })">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
            
        </div>
        <h4 class="panel-title">PO Lookup</h4>
    </div>
    <div class="panel-body" id="PurchaseOrderSummaryListViewModel">
        @Html.Partial("../PurchaseOrder/Search/Table", Model)
    </div>
</div>
