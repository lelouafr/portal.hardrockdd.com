@model portal.Models.Views.Web.UserAccessListViewModel
@{
    ViewBag.TableRow = "True";
    var tableId = string.Format("table_{0}", Guid.NewGuid());
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    string tableKey = "";
}
<table class="table"
       id="@tableId"
       data-urlselect="@Url.Action("Panel", "WebFormUserLogs")"
       data-dstselect="reviewPanel"
       data-dstselectvalidate="false"
       data-urlcreate="@Url.Action("Add", "WebFormUsers")"
       data-urldelete="@Url.Action("Delete", "WebFormUsers")"
       data-urlupdate="@Url.Action("Update", "WebFormUsers")"
       data-tablekey="@tableKey">
    <thead>
        <tr>
            <th>@Html.LabelForRequired(model => model.List.FirstOrDefault().UserName)</th>
            <th>@Html.LabelForRequired(model => model.List.FirstOrDefault().LastAccessed)</th>
            <th>@Html.LabelForRequired(model => model.List.FirstOrDefault().AccessLevel)</th>
            <th></th>

            @if (!viewOnly)
            {
                <th></th>
            }
            </tr>
    </thead>
    <tbody>
        @foreach (var request in Model.List.OrderBy(f => f.UserName).ToList())
        {
            @Html.Partial("../Administration/Web/Access/Users/List/TableRow", request)
        }
    </tbody>
</table>
<script>
    $(document).ready(function () {
        $('#@tableId').DataTable({
            autoWidth: false,
            paging: true,
            pageLength: 10,
            ordering: true,
            columnDefs: [
                { targets: 3, visible: false },
                { targets: 4, orderable: false }
            ],
            fnInitComplete: function () {
                Table().init($('#@tableId'));
            },
            order: [
                [0, "asc"]
            ],
        });
    });

</script>