@model portal.Areas.Payroll.Models.Ticket.Job.FormViewModel

<style>
</style>

<div class="row rowform">
    <div class="col-xl-7">
        @Html.Partial("Form/Job/Info/_Panel", Model.Ticket)
    </div>
    <div class="col-xl-5">
        @Html.Partial("Form/Log/_Panel", Model.StatusLogs)
        @Html.Action("Panel", "Explorer", new { Area = "Attachment", uniqueAttchID = Model.UniqueAttchID })
    </div>
</div>
<div class="row rowform">
    <div class="col-xl-6">
        @Html.Partial("Form/Job/Employee/_Panel", Model.Employees)
    </div>
    <div class="col-xl-6">
        @Html.Partial("Form/Job/Task/_Panel", Model.Phases)
    </div>
</div>