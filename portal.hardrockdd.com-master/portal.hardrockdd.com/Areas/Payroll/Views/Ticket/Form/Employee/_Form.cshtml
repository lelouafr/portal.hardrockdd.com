@model portal.Areas.Payroll.Models.Ticket.Employee.FormViewModel

<div class="row rowform">
    <div class="col-xl-7">
        @Html.Partial("Form/Employee/Info/_Panel", Model.Ticket)
    </div>
    <div class="col-xl-5">
        @Html.Partial("Form/Log/_Panel", Model.StatusLogs)
        @Html.Action("Panel", "Explorer", new { Area = "Attachment", uniqueAttchID = Model.UniqueAttchID })
    </div>
</div>
<div class="row rowform">
    <div class="col-xl-12">
        @Html.Partial("Form/Employee/PerDiem/_Panel", Model.PerDiems)
    </div>
</div>
<div class="row rowform">
    <div class="col-xl-12">
        @Html.Partial("Form/Employee/Entry/_Panel", Model.Entries)
    </div>
</div>