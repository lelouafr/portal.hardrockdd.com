@model  portal.Areas.Project.Models.Bid.Package.AwardListViewModel
@{
    ViewBag.TableRow = "True";
    var tableId = string.Format("table_{0}", Guid.NewGuid());
    var tableLayout = new TableLayoutModel(this);
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    string tableKey = tableLayout.TableKey;
}

<table class="table"
       id="@tableId"
       data-urlupdate="@Url.Action("PackageAwardUpdate", "Bid")"
       data-urlvalidate="@Url.Action("PackageAwardValidate", "Bid")"
       data-urlselect="@Url.Action("PackageAwardBoreListPanel", "Bid")"
       data-dstselect="PackageJobSetupBoreListPanel"
       data-dstselectvalidate="true"
       data-tablekey="@tableKey">
    <thead>
        <tr class="">
            <th width="150px">@Html.LabelFor(model => model.List.FirstOrDefault().Description)</th>
            <th width="12%">@Html.LabelFor(model => model.List.FirstOrDefault().DivisionId)</th>
            <th width="16%">@Html.LabelFor(model => model.List.FirstOrDefault().CustomerId)</th>
            <th width="12%">@Html.LabelFor(model => model.List.FirstOrDefault().CustomerReference)</th>
            <th width="10%">@Html.LabelFor(model => model.List.FirstOrDefault().DirtLFPrice)</th>
            <th width="10%">@Html.LabelFor(model => model.List.FirstOrDefault().RockLFPrice)</th>
            <th width="10%">@Html.LabelFor(model => model.List.FirstOrDefault().AwardStatus)</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model.List)
        {
            @Html.Partial("Forms/Package/Award/List/_TableRow", obj)
        }
    </tbody>
</table>

<script>
    $('#@tableId').ready(function () {
        Table().init($('#@tableId'));
    });
</script>