@model portal.Areas.Project.Models.Bid.Package.JobViewModel
@{
    var tableLayout = new TableLayoutModel(this);
    var viewOnly = bool.TryParse(ViewBag.ViewOnly?.ToString(), out bool viewOnlyOut) ? viewOnlyOut : false;
    var entityKey = tableLayout.TableKey;
}
<tr data-entitykey="@entityKey">
    <td>
        @Html.AntiForgeryToken()
        @Html.HiddenFor(m => m.BDCo, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.BidId, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.PackageId, new { @id = Guid.NewGuid() })
        @Html.HiddenFor(m => m.JCCo, new { @id = Guid.NewGuid() })
        @Html.EditorFor(m => m.Description)
    </td>
    <td>
        @Html.EditorFor(m => m.DivisionId, new { ViewOnly = true })
    </td>
    <td>
        @Html.EditorFor(m => m.CustomerId)
    </td>
    <td>
        @Html.EditorFor(m => m.CustomerReference)
    </td>
    <td>
        @Html.EditorFor(m => m.JobId)
    </td>
    <td>
        @Html.EditorFor(m => m.DirtLFPrice, new { ViewOnly = Model.BidTypeId != DB.BidTypeEnum.QuickBid })
    </td>
    <td>
        @Html.EditorFor(m => m.RockLFPrice, new { ViewOnly = Model.BidTypeId != DB.BidTypeEnum.QuickBid })
    </td>
    <td>
        @Html.EditorFor(m => m.AwardStatus)
    </td>
</tr>
