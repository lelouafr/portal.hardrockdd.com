@model portal.Areas.HumanResource.Models.PositionRequest.RequestFormViewModel
@{
    var id = Guid.NewGuid().ToString().Replace("-", "");
    var panelId = string.Format("panel_{0}", Guid.NewGuid());
}
<div class="panel panel-inverse"
     data-reloadurl="@Url.Action("Form", "PositionRequest", new { area = "HumanResource", hrco = Model.HRCo, requestId = Model.RequestId })">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
        </div>
        <h4 class="panel-title">Position Form (@Model.RequestId)</h4>
    </div>
    <div class="panel-body" id="@panelId">
        @Html.Partial("Form/_Form", Model)
    </div>
</div>